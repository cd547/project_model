<script type="text/javascript">
    //创建项目分析
    function createalert(pro_num) {
        $('#myModal1').modal('show');
        $('.modal-title').empty();
        $('.modal-title').html('添加'+'<strong><?php echo $this->project_data[0]['pro_name']?></strong>'+'项目分析');
        $('#content').empty();
        htmltext="<form id=\"demo-form2\" data-parsley-validate=\"\" class=\"form-horizontal form-label-left\" novalidate=\"\">\n" +
            "<div class=\"form-group\">\n" +
            "     <label class=\"control-label col-md-2 col-sm-2 col-xs-12\">思考</label>\n" +
            "     <div class=\"col-md-10 col-sm-10 col-xs-12\">\n" +
            "         <textarea class=\"form-control\" rows=\"4\" id=\"addproblem\"></textarea>\n" +
            "     </div>\n" +
            "</div>\n" +
            "<div class=\"form-group\">\n" +
            "      <label class=\"control-label col-md-2 col-sm-2 col-xs-12\">分析</label>\n" +
            "      <div class=\"col-md-10 col-sm-10 col-xs-12\">\n" +
            "         <textarea class=\"form-control\" rows=\"4\" id=\"addsolve\"></textarea>\n" +
            "      </div>\n" +
            "</div>\n" +
            "<div class=\"ln_solid\"></div>\n" +
            "<div class=\"form-group\">\n" +
            "      <div class=\"col-md-6 col-sm-6 col-xs-12 col-md-offset-3\">\n" +
            "         <a class=\"btn btn-primary\" type=\"button\" data-dismiss=\"modal\">取消</a>\n" +
            "         <a type=\"submit\" class=\"btn btn-success\" data-dismiss=\"modal\" onclick=\"createanalysis()\">提交</a>\n" +
            "      </div>\n" +
            "</div>\n" +
            "</form>";
        $('#content').html(htmltext);
    }
    //增加项目分析
    function createanalysis()
    {

        var addproblem=$('#addproblem').val().trim(), addsolve=$('#addsolve').val().trim();
        if(addproblem==''||addsolve=='')
        {
            alert("创建项目分析<?php echo $this->project_data[0]['pro_name']?>失败！\n思考或分析不能为空！");
            return;
        }
        $.ajaxSetup({async: true});
        $.post("/project/proanalysis/ajaxadd",{pro_num:'<?php echo $this->project_data[0]['pro_num']?>',addproblem:addproblem,addsolve:addsolve},
            function(returnedData,status){
                var result=returnedData;
                if(result>0){
                    updatepage(1);
                    new PNotify({
                        title: '创建成功！',
                        text: "创建项目分析<?php echo $this->project_data[0]['pro_name']?>成功！",
                        type: 'success',
                        styling: 'bootstrap3'
                    });

                }
                else//网络错误
                {alert("创建项目分析失败");}
            });
    }

    //刷新分页
    function updatepage(page){
        $.ajaxSetup({async: true});
        $.post("/project/proanalysis/ajaxpage",{query:'<?php echo $this->project_data[0]['pro_num']?>',page:page},
            function(returnedData,status){
                var result=returnedData;
                if(result!=null){
                    $('#pro_analysis_list').empty();
                    $('#pro_analysis_list').html(result);
                    init_DataTables();//必须重新初始化表，不然没有打印功能
                    $('[data-toggle="tooltip"]').tooltip({
                        container: 'body'
                    });
                }
                else//网络错误
