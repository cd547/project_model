            function(returnedData,status){
                var result=returnedData;
                if(result>0){
                    updatepage(1);
                    new PNotify({
                        title: '删除成功！',
                        text: "删除项目分析<?php echo $this->project_data[0]['pro_name']?>_"+id+"成功！",
                        type: 'success',
                        nonblock: {
                            nonblock: true
                        },
                        styling: 'bootstrap3'
                    });


                }
                else//网络错误
                {alert("删除项目分析失败");}
            });
    }
